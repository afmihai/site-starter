{
  "name": "Gentlab",
  "private": true,
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "node devServer.js",
    "clean": "rimraf dist/*",
    "build:dist": "webpack --config ./webpack/webpack.config.js --env=dist",
    "build:server": "webpack --config ./webpack/webpack.config.js --env=server",
    "build": "npm run clean && npm run build:dist && npm run build:server",
    "deploy": "git pull origin master && npm run build && pm2 restart gentlab",
    "deploy:initial": "npm install && npm run build && pm2 start index.js --name gentlab -x -- --environment=production",
    "deploy:withInstall": "git pull origin master && npm install && npm run build && pm2 restart gentlab",
    "deploy:withUpdate": "git pull origin master && npm update && npm run build && pm2 restart gentlab"
  },
