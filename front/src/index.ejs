    <base href="/">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <%- metaTagsHtml %>
    <link rel="shortcut icon" type="image/png" href="assets/favicon.png"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <link href="app.<%- hash %>.css" rel="stylesheet" />
</head>
<body>
    <div id="app"><%- appHtml %></div>
    <script type="text/javascript"><%- `window.__initial_state__ = ${JSON.stringify(initialState)}` %></script>
    <script type="text/javascript" src="app.<%- hash %>.js"></script>
    <script type="application/ld+json">
        {
          "@context": "http://schema.org",
          "@type": "Organization",
          "name": "<%= sails.config.publicUrl %>",
          "url": "<%= sails.config.siteName %>",
          "contactPoint": [{
            "@type": "ContactPoint",
            "telephone": "+4021-367-2570",
            "contactType": "customer service"
          }]
        }
    </script>
    <script>
